ALL_CPPFLAGS += $(CPPFLAGS) -MD -MP $(DEFINES) $(INCLUDES)
ALL_RESFLAGS += $(RESFLAGS) $(DEFINES) $(INCLUDES)
LDDEPS +=
ALL_LDFLAGS += $(LDFLAGS) -L/opt/homebrew/Cellar/sfml/2.6.0/lib -m64 -dynamiclib -Wl,-install_name,@rpath/libEngine.dylib
